<!-- Header Start -->
    <header class="header">
        {{ page.top_header }}
        <div class="logo-lang">
            <div class="alta-boxed-width">
                <div class="alta-row">
                    <div class="alta-col-lg-12">
                        <div class="logo-lang-inner">
                            <div class="logo">
                                {{ page.header }}
                            </div>
                            <div class="hamburger-container">
                                <ul class="hamburger">
                                    <li></li>
                                    <li></li>
                                    <li></li>
                                </ul>
                            </div>
                            <div class="language">
                                {{ page.secondary_menu }}
							</div>
							<div class="signupbtn">
                               {{ page.sign_up_button }}
							</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="main-menu">
            <div class="alta-boxed-width">
                <div class="alta-row">
                    <div class="alta-col-lg-12">
                        {{ page.primary_menu }}
                    </div>
                </div>
            </div>
        </div>
        <div class="breadcrumb">
            <div class="alta-boxed-width">
                <div class="alta-row">
                    <div class="alta-col-lg-12">
                        {{ page.breadcrumb }}
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- Header End -->