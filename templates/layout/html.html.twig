{% block body %}
  {%
    include "@myComponents/navigation/skip-link/skip-link.twig" with {
      modifier_class: 'visually-hidden visually-hidden--focusable',
      url: '#main-content',
      text: 'Skip to main content'|t
    } only
  %}
  
  {{ page_top }}
  {{ page }}
  {{ page_bottom }}
  <js-bottom-placeholder token="{{ placeholder_token|raw }}">
{% endblock %}
